{
	"name": "pcb-tht-holder",
	"version": "1.9.0",
	"type": "module",
	"main": "build/index.js",
	"engines": {
		"node": ">=22.0.0",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"dev": "vite dev --host",
		"prebuild": "cp src/service-worker.template.js public/service-worker.js && replace-in-file __VERSION__ v$npm_package_version public/service-worker.js",
		"build": "vite build",
		"preview": "vite preview",
		"ts:check": "svelte-check --tsconfig ./tsconfig.json",
		"format:check": "prettier --check .",
		"format:fix": "prettier --write . | grep -v 'unchanged' | sed G",
		"lint:check": "eslint .",
		"lint:fix": "eslint --fix .",
		"npm:reinstall": "rm -rf ./node_modules && rm -f ./package-lock.json && npm i && npm i",
		"all": "node --run format:fix && node --run lint:fix && node --run ts:check && node --run build"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/postcss": "^4.1.11",
		"@tailwindcss/vite": "^4.1.11",
		"@tsconfig/svelte": "^5.0.4",
		"@types/three": "^0.177.0",
		"@typescript-eslint/eslint-plugin": "^8.35.0",
		"@typescript-eslint/parser": "^8.35.0",
		"eslint": "^9.30.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-svelte": "^3.10.1",
		"eslint-plugin-unicorn": "^59.0.1",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"replace-in-file": "^8.3.0",
		"svelte-check": "^4.2.2",
		"svelte-eslint-parser": "^1.2.0",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.11",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@iconify/svelte": "^5.0.0",
		"@threlte/core": "^8.0.5",
		"@threlte/extras": "^9.4.0",
		"canvas": "^3.1.2",
		"dayjs": "^1.11.13",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.8.2",
		"konva": "^9.3.20",
		"nanoid": "^5.1.5",
		"svelte": "^5.34.9",
		"svelte-konva": "^1.0.0-next.2",
		"svelte-persisted-store": "^0.12.0",
		"three": "^0.177.0",
		"three-bvh-csg": "^0.0.17",
		"zod": "^3.25.67"
	}
}
